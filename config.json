{
    "fontsize": 12,
    "font_color": "#CCCCCC",
    "poverty_weight": 1.0,
    "food_weight": 1.0,
    "vehicle_weight": 1.0,
    "vehicle_num_toggle": false,
    "show_programs": false,
    "normalize": true,
    "scale_max": 35,
    "map_display": {
        "height": 800,
        "map_style": "satellite-streets",
        "zoom": 8.5,
        "opacity": 0.33
    },
    "file_paths": {
        "tract_geo": "data/tracts_new.pkl",
        "food_insecurity": "data/food_insecurity.csv",
        "poverty": "data/poverty.csv",
        "vehicle": "data/vehicle.csv",
        "county_seats": "data/counties.csv",
        "acs": "data/full_acs_data.pkl",
        "programs": "data/shnwnc_facilities.csv"
    },
    "county_seat_marker": {
        "allowoverlap": true,
        "color": "#FFFFFF",
        "size": 8,
        "opacity": 0.75,
        "symbol": "star"
    },
    "client_marker": {
        "allowoverlap": true,
        "color": "#0000CC",
        "size": 16,
        "opacity": 0.5,
        "symbol": "circle"
    },
    "program_marker": {
        "allowoverlap": true,
        "size": 8,
        "opacity": 0.75
    },
    "sliders": {
        "weight": {
            "min": 0.0,
            "max": 1.0,
            "step": 0.01
        },
        "scale_max": {
            "min": 10,
            "max": 100,
            "step": 1
        },
        "map_opacity": {
            "min": 0.1,
            "max": 1.0,
            "step": 0.05
        },
        "font_size": {
            "min": 8,
            "max": 24,
            "step": 2
        },
        "marker_size": {
            "min": 8,
            "max": 48,
            "step": 2
        }
    }
}